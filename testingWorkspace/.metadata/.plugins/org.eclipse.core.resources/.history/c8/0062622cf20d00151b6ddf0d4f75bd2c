package xmetamodel.implementation;

import xmetamodel.*;
import com.salexandru.xcorex.interfaces.Group;
import xcorexview.metrics.classes.NumberOfLines;
import xcorexview.metrics.classes.Name;
import xcorexview.metrics.Groups.ListClassMethods;


public class XClassImpl implements XClass {
    private org.eclipse.jdt.core.IType underlyingObj_;

    private static final NumberOfLines NumberOfLines_INSTANCE = new NumberOfLines();
    private static final Name Name_INSTANCE = new Name();
    private static final ListClassMethods ListClassMethods_INSTANCE = new ListClassMethods();


    public XClassImpl(org.eclipse.jdt.core.IType underlyingObj) {
        underlyingObj_ = underlyingObj;
    }
    @Override
    public org.eclipse.jdt.core.IType getUnderlyingObject() {
        return underlyingObj_;
    }
@Override
public java.lang.Integer numberOfLines() {
    return NumberOfLines_INSTANCE.compute(this);
}
@Override
public java.lang.String name() {
    return Name_INSTANCE.compute(this);
}
@Override
public Group<XMethod> listClassMethods() {
  return ListClassMethods_INSTANCE.buildGroup(this);
}
    public boolean equals(Object obj) {
        if (null == obj || !(obj instanceof XClassImpl)) {
           return false;
        }
        XClassImpl iObj = (XClassImpl)obj;
        if (null == underlyingObj_ || null == iObj.getUnderlyingObject()) {
           return true;
        }
        return underlyingObj_.equals(iObj);
    }
}
