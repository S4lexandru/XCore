package xmetamodel.implementation;

import xmetamodel.*;
import com.salexandru.corex.interfaces.Group;
import xcorexview.metrics.packages.Name;
import xcorexview.metrics.Groups.ListPackageClasses;


public class XPackageImpl implements XPackage {
    private org.eclipse.jdt.core.IPackageFragment underlyingObj_;

    private static final Name Name_INSTANCE = new Name();
    public static final ListPackageClasses ListPackageClasses_INSTANCE = new ListPackageClasses();


    public XPackageImpl(org.eclipse.jdt.core.IPackageFragment underlyingObj) {
        underlyingObj_ = underlyingObj;
        ListPackageClasses_INSTANCE.buildGroup(this);
    }
    @Override
    public org.eclipse.jdt.core.IPackageFragment getUnderlyingObject() {
        return underlyingObj_;
    }
@Override
public java.lang.String name() {
    return Name_INSTANCE.compute(this);
}
@Override
public Group<XClass> listPackageClasses() {
    return ListPackageClasses_INSTANCE.getGroup();
}
    public boolean equals(Object obj) {
        if (null == obj || !(obj instanceof XPackageImpl)) {
           return false;
        }
        XPackageImpl iObj = (XPackageImpl)obj;
        if (null == underlyingObj_ || null == iObj.getUnderlyingObject()) {
           return true;
        }
        return underlyingObj_.equals(iObj);
    }
}
